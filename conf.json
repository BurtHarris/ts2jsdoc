{
    "plugins": [
        "plugin.js",
        "plugins/markdown"
    ],
    "opts": {
        "readme": "README.md",
        "template": "template"
    },
    "typescript": {
        "target": "ES5",
        "module":  "CommonJS"
    },
    "templates": {
        "systemName": "ts2jsdoc",
        "theme": "default",
        "navType": "vertical",
        "inverseNav": false,
        "tableOfContents": true,
        "linenums": true,
        "collapseSymbols": true,
        "footer": "Hey",
        "copyright": "SPATools Copyright &copy; 2015 The contributors to the JSDoc3, ts2jsdoc projects.",
        "outputSourceFiles": false,
        "outputSourcePath": false
    },
    "source": {
        "includePattern": "(\\.d)?\\.ts$"
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    }
}
